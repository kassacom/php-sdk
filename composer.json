{
    "name": "kassacom/php-sdk",
    "description": "Kassa.com PHP SDK",
    "keywords": ["payment", "payments", "pay", "cards", "money", "sdk", "kassa.com"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kassa.com Developers",
            "email": "dev@kassa.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.0|^2.0|^3.0",
        "guzzlehttp/psr7": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "guzzlehttp/guzzle": "^6.0.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Guzzle Api Transport"
    },
    "autoload": {
        "psr-4": {
            "KassaCom\\SDK\\": "src/",
            "Tests\\KassaCom\\SDK\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7.x-dev"
        }
    }
}
